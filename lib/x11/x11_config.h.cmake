/* Configure build time options of GLFW */

/* Define this to 1 to XRandR is available */ 
#cmakedefine _GLFW_HAS_XRANDR      1 
/* Define this to 1 to Xf86VidMode is available */ 
#cmakedefine _GLFW_HAS_XF86VIDMODE 1
/* Define this to 1 if glxGetProcAddress is available */
#cmakedefine _GLFW_HAS_GLXGETPROCADDRESS 1 
/* Define this to 1 if glxGetProcAddressARB is available */
#cmakedefine _GLFW_HAS_GLXGETPROCADDRESSARB 1 
/* Define this to 1 if glxGetProcAddressEXT is available */
#cmakedefine _GLFW_HAS_GLXGETPROCADDRESSEXT 1 
/* Define this to 1 if Pthreads are available */ 
#cmakedefine _GLFW_HAS_PTHREAD     1 
#cmakedefine _REENTRANT 
/* Define this to 1 if Sched Yield is available */ 
#cmakedefine _GLFW_HAS_SCHED_YIELD 1
/* Define this to 1 if dlopen is available */ 
#cmakedefine _GLFW_HAS_DLOPEN      1 
/* 
 Define this to 1 if Sysconf is available and can 
 be used to get the number of CPU available 
*/ 
#cmakedefine _GLFW_HAS_SYSCONF     1
/* 
 Define this to 1 if Sysctl is available 
 and can be used to get the number of CPU available 
*/
#cmakedefine _GLFW_HAS_SYSCTL      1


